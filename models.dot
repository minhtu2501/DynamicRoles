digraph models_diagram {
	graph[overlap=false, splines=true, bgcolor="none"]
	"Role" [shape=Mrecord, label="{Role|id :int(11)\lname :varchar(255)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"User" [shape=Mrecord, label="{User|id :int(11)\lname :varchar(255)\lemail :varchar(255)\lencrypted_password :varchar(255)\lreset_password_token :varchar(255)\lreset_password_sent_at :datetime\lremember_created_at :datetime\lsign_in_count :int(11)\lcurrent_sign_in_at :datetime\llast_sign_in_at :datetime\lcurrent_sign_in_ip :varchar(255)\llast_sign_in_ip :varchar(255)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"Part" [shape=Mrecord, label="{Part|id :int(11)\lname :varchar(255)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"RolesUser" [shape=Mrecord, label="{RolesUser|user_id :int(11)\lrole_id :int(11)\l}"]
	"PermissionsUser" [shape=Mrecord, label="{PermissionsUser|user_id :int(11)\lpermission_id :int(11)\l}"]
	"Plan" [shape=Mrecord, label="{Plan|id :int(11)\lnumber :varchar(255)\lpart_id :int(11)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"Permission" [shape=Mrecord, label="{Permission|id :int(11)\lsubject_class :varchar(255)\laction :varchar(255)\lname :varchar(255)\ldescription :text\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"Drawing" [shape=Mrecord, label="{Drawing|id :int(11)\lsheet_number :varchar(255)\lpart_id :int(11)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"PermissionsRole" [shape=Mrecord, label="{PermissionsRole|role_id :int(11)\lpermission_id :int(11)\l}"]
	"Role" -> "RolesUser" [arrowtail=odot, arrowhead=crow, dir=both color="#392722"]
	"Role" -> "PermissionsRole" [arrowtail=odot, arrowhead=crow, dir=both color="#3AA186"]
	"Role" -> "Permission" [arrowtail=crow, arrowhead=crow, dir=both color="#847679"]
	"User" -> "RolesUser" [arrowtail=odot, arrowhead=crow, dir=both color="#99A70B"]
	"User" -> "Role" [arrowtail=crow, arrowhead=crow, dir=both color="#803BB2"]
	"User" -> "PermissionsUser" [arrowtail=odot, arrowhead=crow, dir=both color="#2D6536"]
	"User" -> "Permission" [arrowtail=crow, arrowhead=crow, dir=both color="#0EC2D2"]
	"User" -> "Permission" [label="role_permissions", arrowtail=crow, arrowhead=crow, dir=both color="#0DE4EE"]
	"Part" -> "Drawing" [arrowtail=odot, arrowhead=crow, dir=both color="#0C502D"]
	"Permission" -> "PermissionsRole" [arrowtail=odot, arrowhead=crow, dir=both color="#F8F40F"]
	"Permission" -> "PermissionsUser" [arrowtail=odot, arrowhead=crow, dir=both color="#B03689"]
}
